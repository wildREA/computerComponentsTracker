    <sys:String x:Key="EnglishLanguage">Englisch</sys:String>
    <sys:String x:Key="FrenchLanguage">Französisch</sys:String>
    <sys:String x:Key="HebrewLanguage">Hebräisch</sys:String>
    <sys:String x:Key="HindiLanguage">Hindi</sys:String>
    <sys:String x:Key="KoreanLanguage">Koreanisch</sys:String>
    <sys:String x:Key="ItalianLanguage">Italienisch</sys:String>
    <sys:String x:Key="KurdishLanguage">Kurdish</sys:String>

<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib">
    <!-- MainWindow -->
    <sys:String x:Key="SoftwareTitle">Computerkomponenten-Tracker</sys:String>
    <sys:String x:Key="SoftwareName">Task-Manager</sys:String>
    <sys:String x:Key="MonitorTab">Monitor</sys:String>
    <sys:String x:Key="SettingsTab">Einstellungen</sys:String>

    <!-- Strings.xaml -->
    <sys:String x:Key="TrackerTitle">Systemmonitor</sys:String>
    <sys:String x:Key="CPUUsage">CPU-Auslastung:</sys:String>
    <sys:String x:Key="GPUUsage">GPU-Auslastung:</sys:String>
    <sys:String x:Key="RAMUsage">RAM-Auslastung:</sys:String>
    <sys:String x:Key="DiskUsage">Festplattenauslastung:</sys:String>
    <sys:String x:Key="Battery">Batterie:</sys:String>
    <sys:String x:Key="NetworkAdapter">Netzwerkadapter:</sys:String>
    <sys:String x:Key="Notice">Falls der Netzwerkadapter falsch ist, muss das Programm neu gestartet werden.</sys:String>

    <!-- Settings.xaml -->
    <sys:String x:Key="SettingsTitle">Einstellungen</sys:String>
    <sys:String x:Key="ChangeSettings">Ändern Sie die Einstellungen der Anwendung</sys:String>
    <sys:String x:Key="ChangeTheme">Thema</sys:String>
    <sys:String x:Key="ChangeLanguage">Sprache</sys:String>
    <sys:String x:Key="ChangeNetworkAdapter">Netzwerkadapter</sys:String>
    <sys:String x:Key="ChangeRefreshRate">Aktualisierungsrate</sys:String>
    <sys:String x:Key="LightTheme">Hell</sys:String>
    <sys:String x:Key="DarkTheme">Dunkel</sys:String>

<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:sys="clr-namespace:System;assembly=System.Runtime">
    <sys:String x:Key="SettingsTitle">Settings</sys:String>
    <sys:String x:Key="ChangeSettings">Change the settings for the application</sys:String>
    <sys:String x:Key="ChangeTheme">Change the theme</sys:String>
    <sys:String x:Key="ChangeLanguage">Change the language</sys:String>
    <sys:String x:Key="ChangeNetworkAdapter">Change the network adapter</sys:String>
    <sys:String x:Key="ChangeRefreshRate">Change the refresh rate</sys:String>
    <sys:String x:Key="LightTheme">Light</sys:String>
    <sys:String x:Key="DarkTheme">Dark</sys:String>
    <sys:String x:Key="EnglishLanguage">English</sys:String>
    <sys:String x:Key="RussianLanguage">Russian</sys:String>
    <sys:String x:Key="AutoNetwork">Auto</sys:String>
    <sys:String x:Key="EthernetNetwork">Ethernet</sys:String>
    <sys:String x:Key="WiFiNetwork">Wi-Fi</sys:String>
    <sys:String x:Key="SecondRefreshRate">Seconds</sys:String>
    <sys:String x:Key="MillisecondRefreshRate">Milliseconds</sys:String>
</ResourceDictionary>

